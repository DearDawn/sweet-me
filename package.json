{
  "name": "sweet-me",
  "version": "0.2.55",
  "description": "可爱糖的React&TS组件库（2020-09-02）",
  "scripts": {
    "clean": "rm -rf dist && rm -rf .parcel-cache",
    "build": "npm run clean && rollup -c",
    "start": "npm run build && parcel src/page/index.html",
    "build-page": "npm run build && parcel build --target page",
    "prepublishOnly": "npm run build && npm version patch",
    "postpublish": "node ./scripts/initChangeLog.js"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "targets": {
    "page": {
      "source": "src/page/index.html",
      "distDir": "./dist/page",
      "isLibrary": false,
      "publicUrl": "./"
    }
  },
  "sideEffects": [
    "**/*.less"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/DearDawn/sweet-me.git"
  },
  "files": [
    "README.md",
    "CHANGE_LOG.md",
    "dist/",
    "package.json"
  ],
  "keywords": [
    "sweet-me"
  ],
  "author": "tangjingchun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DearDawn/sweet-me/issues"
  },
  "homepage": "https://github.com/DearDawn/sweet-me#readme",
  "dependencies": {
    "clsx": "^2.1.0",
    "github-markdown-css": "^5.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.12.0",
    "@parcel/core": "^2.12.0",
    "@parcel/transformer-less": "^2.12.0",
    "@rollup/plugin-eslint": "^9.0.5",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "less": "^4.2.0",
    "parcel": "^2.12.0",
    "parcel-transformer-markdown": "^3.0.1",
    "postcss": "^8.4.32",
    "postcss-modules": "^4.3.1",
    "rollup": "^2.79.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.3.3",
    "webpack": "^4.44.1"
  }
}
